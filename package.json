{
  "name": "vue-tianditu",
  "version": "2.6.3",
  "description": "vue-tianditu",
  "author": "SoulLyoko <519052532@qq.com>",
  "repository": "https://github.com/SoulLyoko/vue-tianditu.git",
  "homepage": "https://github.com/SoulLyoko/vue-tianditu#readme",
  "private": false,
  "license": "MIT",
  "main": "lib/vue-tianditu.umd.js",
  "types": "lib/vue-tianditu.d.ts",
  "keywords": [
    "vue",
    "tianditu",
    "map",
    "skymap",
    "tianmap"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "docs:dev": "npm run cp && vitepress dev docs",
    "docs:build": "npm run cp && vitepress build docs",
    "docs:serve": "npm run cp && vitepress serve docs",
    "lint": "eslint --ext .ts,.vue .",
    "prepare": "husky install",
    "dts": "vue-dts-gezn packages/**/*.vue",
    "cp:readme": "ncp README.md docs/index.md",
    "cp:changelog": "ncp CHANGELOG.md docs/CHANGELOG.md",
    "cp": "npm run cp:readme && npm run cp:changelog"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "vue-demi": "^0.11.4"
  },
  "peerDependencies": {
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "3.2.20",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "ncp": "^2.0.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "vite": "2.6.13",
    "vitepress": "^0.20.1",
    "vue": "^3.2.20",
    "vue-dts-gen": "^0.3.0",
    "vue-tsc": "^0.28.10",
    "vue2": "npm:vue@2",
    "vue3": "npm:vue@3"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
