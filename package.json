{
  "name": "vue-tianditu",
  "version": "2.7.6",
  "description": "vue-tianditu",
  "author": "SoulLyoko <519052532@qq.com>",
  "repository": "https://github.com/SoulLyoko/vue-tianditu.git",
  "homepage": "https://github.com/SoulLyoko/vue-tianditu#readme",
  "private": false,
  "license": "MIT",
  "main": "lib/index.umd.js",
  "types": "index.d.ts",
  "keywords": [
    "vue",
    "tianditu",
    "map",
    "skymap",
    "tianmap"
  ],
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev2": "node scripts/dev2.js",
    "build": "node scripts/build.js",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "typecheck": "vue-tsc --noEmit",
    "serve": "vite preview",
    "docs:dev": "node scripts/docs_dev.js",
    "docs:build": "node scripts/docs_build.js",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint --ext .ts,.vue .",
    "prepare": "husky install",
    "precommit": "node scripts/precommit.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "vue-demi": "^0.12.5"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.6.0",
    "vue": "^2.6.0 || >=3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/composition-api": "1.6.1",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "execa": "^4.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "ncp": "^2.0.0",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.6.4",
    "vite": "2.9.8",
    "vite-plugin-vue2": "^1.9.0",
    "vitepress": "^0.22.4",
    "vue": "^3.2.33",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.34.11"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
