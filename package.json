{
  "name": "vue-tianditu",
  "version": "2.7.6",
  "upperName": "VueTianditu",
  "description": "vue-tianditu",
  "author": "SoulLyoko <519052532@qq.com>",
  "repository": "https://github.com/SoulLyoko/vue-tianditu.git",
  "homepage": "https://github.com/SoulLyoko/vue-tianditu#readme",
  "private": false,
  "license": "MIT",
  "keywords": [
    "vue",
    "tianditu",
    "map",
    "skymap",
    "tianmap"
  ],
  "files": [
    "dist",
    "es",
    "lib",
    "global.d.ts"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "style": "dist/index.css",
  "scripts": {
    "build": "yarn switch:3 && yarn typecheck && esno scripts/build.ts",
    "build:global": "esno scripts/global.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ci": "yarn build && yarn docs:build && yarn test:all",
    "dev": "yarn switch:3 && vite",
    "dev:2": "yarn switch:2 && vite",
    "dev:2.7": "yarn switch:2.7 && vite",
    "docs:dev": "yarn switch:3 && vitepress dev packages",
    "docs:build": "yarn switch:3 && vitepress build packages",
    "docs:serve": "yarn switch:3 && vitepress serve packages",
    "lint": "eslint --ext .ts,.vue .",
    "prepare": "husky install",
    "release": "bump && esno scripts/release.ts",
    "serve": "yarn switch:3 && vite preview",
    "switch:2": "esno scripts/switch.ts 2",
    "switch:2.7": "esno scripts/switch.ts 2.7",
    "switch:3": "esno scripts/switch.ts 3",
    "test": "vitest",
    "test:2": "yarn switch:2 && vitest run",
    "test:2.7": "yarn switch:2.7 && vitest run",
    "test:3": "yarn switch:3 && vitest run",
    "test:all": "yarn test:2 && yarn test:2.7 && yarn test:3",
    "test:coverage": "vitest --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "mitt": "^3.0.0",
    "vue-demi": "^0.13.11"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.7.0",
    "vue": "^2.6.0 || >=3.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.10",
    "@types/node": "^18.11.13",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/composition-api": "^1.7.1",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "sass": "^1.55.0",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vite-plugin-vue2": "^2.0.2",
    "vitepress": "1.0.0-alpha.31",
    "vitest": "^0.25.7",
    "vue": "^3.2.45",
    "vue-template-compiler": "^2.7.14",
    "vue-tsc": "^1.0.13"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
